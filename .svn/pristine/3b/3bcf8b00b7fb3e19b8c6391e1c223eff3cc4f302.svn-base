#other-contain{margin:0 auto;}
#other-head{
	margin:0 auto;
	width: 1040px;

    box-shadow: 4px 4px 4px #ACACAC;
}
#other-head-info{height: 220px;padding-top: 46px;text-align:center;background:url("../img/icon/dusk-01.jpg") ;}
#other-avatar{
	width: 100px;
	height: 100px;
	border-radius: 100px;
	border:2px solid #fff;
}
#other-name{
	margin-top: 22px;
	color: #FFF;
	font-size: 18px;
	font-weight: bold;
 	}
#other-nav{
	background-color:#F2F2F2;
	padding:10px 220px 5px 220px;
	box-shadow: 4px 4px 4px #ACACAC;
}
#other-nav a{
	 margin:100px;
	 text-decoration: none;
	 color: #000;}

#other-body{
	width: 1040px;
	margin:0 auto;
}

#other-info{
	width:255px;
	height: 280px;
	background-color: #f2f2f2;
	margin-top: 20px;
	box-shadow: 4px 4px 4px #ACACAC;
	float: left;
}
#other-info-title{
	background-color: #93247a;
	height: 30px;
	color:#fff;
	text-align: center;
	padding-top: 10px;
   }
#other-info p{margin:20px;}
#other-content{
	float: left;
	width: 800px;
	height:1000px;
	margin-top:5px;
}
#other-content-collection{
	background:#f2f2f2 ;
	float: left;
	width: 770px;
	height:1000px;
	margin-top:20px;
	margin-left: 15px;
}
#other-content-collection img{

	width:160px;
	height:220px;
}
#other-content-collection li{	
	float: left;
	text-decoration: none;
	list-style: none;
	margin:15px;}

