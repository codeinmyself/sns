#inform{
    background-color: #f2f2f2;
	padding:22px;
    text-align: left;
    min-height: 500px;
}
#inform table{
	border: 1px solid #cacaca;
	border-collapse: collapse;
	border-left: none;
	border-right: none;
	text-align: left;
	font-weight: bold;
	border-radius: 6px;
	width:100%;
}
#inform table th{
	padding: 12px 16px;
	font-size: 1.6em;
	color: #00CCCC;
}
#inform table td:nth-child(1){
	border-top: 1px solid #cacaca;
	padding: 12px 16px;
	border-radius: 6px;
	width: 80%;
}
#inform table td:nth-child(2){
	border-top: 1px solid #cacaca;
	padding: 12px 16px;
	border-radius: 6px;
	width: 20%;
}
#inform table tr:nth-child(even){
	background-color: #93aca7;
}
#inform .noticeDeleteButton{
	padding:6px 12px;
	background-color: #93247a;
	cursor: pointer;
	border: 1px solid #93247a;
	border-radius: 6px;
	color: #fff;
	font-family: inherit;
}
#inform .noticeDeleteButton:hover{
	background-color: #fff;
	color: #93247a;
}
.informTab{
	padding:6px 15px;
	background-color: #93247a;
	font-size: 1em;
	border-radius: 5px;
	color: #fff;
}
.informTabActive{
	padding:6px 15px;
	background-color: #fff;
	color:#93247a;
	font-size: 1em;
	border-radius: 5px;
}